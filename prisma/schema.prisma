generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-1.0.x"]
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Views {
  slug String @id
  count Int @default(1)
}

model Likes {
  slug String @id
  count Int @default(1)
}
